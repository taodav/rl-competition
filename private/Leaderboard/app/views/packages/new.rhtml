<%= render :partial => 'shared/admin_menu' %>

<% form_for :package, :url => { :action => 'create' }, :html => { :multipart => true } do |f| -%>
  <p><label for="revision">Revision:</label><%= f.text_field :revision %></p>
	<p><%= f.file_field :uploaded_data %></p>
  <p><%= submit_tag "Create Package" %></p>
<% end -%>